<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/main"
            app:startDestination="@id/firstFragment">

    <fragment android:id="@+id/firstFragment" android:name="com.example.dagger2.FirstFragment"
              android:label="fragment_first" tools:layout="@layout/fragment_first">
        <action android:id="@+id/goToSecond" app:destination="@id/secondFragment"/>
    </fragment>
    <fragment android:id="@+id/secondFragment" android:name="com.example.dagger2.SecondFragment"
              android:label="fragment_second" tools:layout="@layout/fragment_second"/>
</navigation>